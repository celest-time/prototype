{
  "name": "celest/prototype",
  "description": "A port of JSR310 to PHP",
  "license": "GPL-2.0",
  "authors": [
    {
      "name": "Steffen Hanikel",
      "email": "steffen.hanikel@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Celest\\": "src/Celest"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Celest\\": "test/Celest"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "tzdata"
    }
  ],
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php" : "^5.6 || ^7.0",
    "celest/prototype-tzdata" : "*",
    "ext-gmp": "*"
  },
  "require-dev": {
    "phpunit/phpunit": ">=4.8"
  }
}